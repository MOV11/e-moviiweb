<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>moviii</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <config-file parent="/manifest" platform="android">
        <uses-permission name="android.permission.ACCESS_COARSE_LOCATION" />
        <uses-permission name="android.permission.ACCESS_FINE_LOCATION" />
        <uses-permission name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    </config-file>
    <platform name="android">
        <preference name="android-minSdkVersion" value="19" />
        <allow-intent href="market:*" />
        <preference name="PLAY_SERVICES_VERSION" value="19" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-geolocation" version="2.1.0" />

    <engine name="android" spec="^7.1.4" />
     <plugin name="cordova-plugin-googlemaps" spec="2.4.6">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyA3WECrzdOEqFDthJAti_ytD9BnPBIMZXc" />
        <variable name="API_KEY_FOR_IOS" value="AIzaSyA3WECrzdOEqFDthJAti_ytD9BnPBIMZXc" />
    </plugin>
    <preference name="phonegap-version" value="cli-8.0.0" />

</widget>
